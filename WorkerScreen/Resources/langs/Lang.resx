  <data name="query_UserCheck" xml:space="preserve">
    <value>SELECT A.EMPNO, A.EMPNM, A.RANK, A.PIN, A.RECSTA, A.POSITION, B.DEPTNM, B.DEPTID, C.WKPL, C.WKPLNM
FROM empmst A INNER JOIN deptmst B ON A.DEPTID = B.DEPTID 
INNER JOIN wkplmst C ON B.WKPL = C.WKPL WHERE PIN = '{0}'</value>
  </data>
