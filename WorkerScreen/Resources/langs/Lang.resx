  <data name="query_RequestCheck" xml:space="preserve">
    <value>SELECT (SELECT COUNT(*) FROM lkmst
WHERE ILSID = '{2}') AS TOTALCNT, COUNT(*) AS CNT,
B.ASTATUS FROM sireq A INNER JOIN silock B ON A.EREQID = B.EREQID 
WHERE DATE(A.STRDT) = '{0}' AND A.WEMPID = '{1}' AND ILSID = '{2}' AND ASTATUS IN {3} FOR UPDATE</value>
  </data>
  <data name="query_SearchLocks" xml:space="preserve">
    <value>SELECT A.LSN, A.LKNM, A.ILSID, A.MAC, A.LKTYP, A.TKID, D.LEVEL, (SELECT ULKEY FROM lkkey WHERE EMPID = '{0}' AND LSN = A.LSN) AS ULKEY,
(SELECT UNCMD FROM lkkey WHERE EMPID = '{0}' AND LSN = A.LSN) AS UNCMD, E.AREANM, H.CDNM, C.LKACT, F.ASTATUS, C.CONDT
FROM lkmst A 
LEFT JOIN lkkey B ON A.LSN = B.LSN
LEFT JOIN tkseg C ON A.TKID = C.TKID
INNER JOIN ilsmst D ON A.ILSID = D.ILSID
LEFT JOIN areamst E ON D.AREA = E.AREA
LEFT JOIN silock F ON A.LSN = F.LSN
LEFT JOIN sireq G ON F.EREQID = G.EREQID
JOIN cdmst H ON D.LEVEL = H.CODE
WHERE {1} AND A.RECSTA = 'A'
GROUP BY LSN
ORDER BY C.CONDT DESC, A.ILSID ASC FOR UPDATE</value>
  </data>
