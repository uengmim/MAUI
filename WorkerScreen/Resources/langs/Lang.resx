  <data name="query_SearchLocksAllOffline" xml:space="preserve">
    <value>SELECT A.LSN, A.LKNM, A.ILSID, A.MAC, A.LKTYP, A.TKID, D.LEVEL, (SELECT ULKEY FROM lkkey WHERE EMPID = 'smartlock' AND LSN = A.LSN) AS ULKEY,
(SELECT UNCMD FROM lkkey WHERE EMPID = 'smartlock' AND LSN = A.LSN) AS UNCMD, E.AREANM, H.CDNM, C.LKACT, F.ASTATUS, C.CONDT
FROM lkmst A 
LEFT JOIN lkkey B ON A.LSN = B.LSN
LEFT JOIN tkseg C ON A.TKID = C.TKID
INNER JOIN ilsmst D ON A.ILSID = D.ILSID
LEFT JOIN areamst E ON D.AREA = E.AREA
LEFT JOIN silock F ON A.LSN = F.LSN
LEFT JOIN sireq G ON F.EREQID = G.EREQID
JOIN cdmst H ON D.LEVEL = H.CODE
WHERE {0} AND A.RECSTA = 'A'
GROUP BY LSN
ORDER BY C.CONDT DESC, A.ILSID ASC FOR UPDATE</value>
  </data>
