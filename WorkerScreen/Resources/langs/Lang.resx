  <data name="query_GetLockOfflineKeys" xml:space="preserve">
    <value>SELECT A.LSN, B.LKNM, A.ULKEY, A.UNCMD, B.ILSID, B.MAC, B.LKTYP,
(SELECT TKID FROM tkseg WHERE LSN = A.LSN ORDER BY CONDT DESC LIMIT 1) as TKID,
(SELECT LKACT FROM tkseg WHERE LSN = A.LSN ORDER BY CONDT DESC LIMIT 1) as LKACT,
C.ILSID, C.`LEVEL`, D.ASTATUS
FROM lkkey A
LEFT JOIN lkmst B ON A.LSN = B.LSN
LEFT JOIN ilsmst C ON B.ILSID = C.ILSID
LEFT JOIN silock D ON A.LSN = D.LSN
LEFT JOIN sireq E ON D.EREQID = E.EREQID
WHERE A.EMPID = 'smartlock' AND A.ulkey &lt;&gt; '' AND A.uncmd &lt;&gt; '' AND {0} AND B.RECSTA = 'A'</value>
  </data>
