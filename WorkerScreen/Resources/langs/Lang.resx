  <data name="query_GetAreas" xml:space="preserve">
    <value>SELECT A.ILSID, A.ILSNM, A.WKPL, A.AREA, B.AREANM, LEVEL FROM ilsmst A INNER JOIN areamst B ON A.AREA = B.AREA WHERE A.WKPL = '{0}' AND A.RECSTA = 'A'</value>
  </data>
  <data name="query_GetEmployees" xml:space="preserve">
    <value>SELECT A.EMPNO, A.EMPNM, A.RANK, A.PIN, B.DEPTNM, B.DEPTID, B.WKPL FROM empmst A INNER JOIN deptmst B ON A.DEPTID = B.DEPTID WHERE B.WKPL = '{0}'</value>
  </data>
